{
    "name": "fengxin/laras",
    "type": "project",
    "description": "A framework based on laravel used swoole.",
    "keywords": [
        "php",
        "swoole",
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "ext-mbstring": "*",
        "ext-redis": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "symfony/console": "5.x-dev",
        "illuminate/database": "^9.0@dev",
        "illuminate/config": "^9.0@dev",
        "symfony/finder": "5.x-dev",
        "nikic/fast-route": "^2.0@dev",
        "fig/http-message-util": "^1.1@dev",
        "swoole/ide-helper": "master",
        "illuminate/translation": "^9.0@dev",
        "illuminate/validation": "^9.0@dev",
        "laravel-lang/lang": "~8.0",
        "doctrine/annotations": "1.11.1",
        "roave/better-reflection": "^4.3",
        "illuminate/events": "^9.0@dev",
        "vlucas/phpdotenv": "5.2.x-dev",
        "nesbot/carbon": "2.31",
        "illuminate/log": "9.x-dev",
        "illuminate/pagination": "9.x-dev",
        "illuminate/hashing": "9.x-dev",
        "illuminate/encryption": "9.x-dev",
        "league/flysystem": "9999999-dev",
        "illuminate/mail": "9.x-dev",
        "illuminate/view": "9.x-dev"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\" : "app/",
            "Laras\\" : "framework/"
        },
        "files": [
            "framework/Support/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
